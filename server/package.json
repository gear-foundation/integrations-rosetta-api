{
  "name": "rosetta-api",
  "version": "1.4.13",
  "description": "Build Once. Integrate Your Blockchain Everywhere.",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "node dist/index.js --config-name development",
    "start:test": "node dist/index.js --config-name testnet",
    "start:vara": "node dist/index.js --config-name vara",
    "start:vara-test": "node dist/index.js --config-name varatest",
    "start:offline": "node dist/index.js --mode offline",
    "watch:dev": "nodemon src/index.ts --config-name development",
    "watch:test": "nodemon src/index.ts --config-name testnet",
    "watch:vara": "nodemon src/index.ts --config-name vara",
    "watch:vara-test": "nodemon src/index.ts --config-name varatest",
    "watch:offline": "nodemon src/index.ts --mode offline",
    "build": "rm -rf dist && tsc"
  },
  "keywords": [
    "openapi-generator",
    "openapi",
    "rosetta",
    "gear"
  ],
  "private": true,
  "dependencies": {
    "@polkadot/api": "10.6.1",
    "@polkadot/api-derive": "10.6.1",
    "@polkadot/rpc-augment": "10.6.1",
    "@polkadot/rpc-core": "10.6.1",
    "@polkadot/types": "10.6.1",
    "@polkadot/types-codec": "10.6.1",
    "@polkadot/types-create": "10.6.1",
    "@polkadot/types-known": "10.6.1",
    "@polkadot/util": "12.1.2",
    "@polkadot/util-crypto": "12.1.2",
    "@substrate/txwrapper-core": "6.0.0",
    "@substrate/txwrapper-polkadot": "6.0.0",
    "body-parser": "1.20.1",
    "camelcase": "6.3.0",
    "commander": "^10.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "express-openapi-validator": "5.0.0",
    "gear-util": "workspace:^",
    "js-yaml": "4.1.0",
    "node-fetch": "2",
    "ono": "7.1.3",
    "openapi-sampler": "^1.0.0-beta.15",
    "rosetta-client": "workspace:^",
    "swagger-ui-express": "4.6.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.57.0",
    "axios": "1.2.1",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "eslint": "8.30.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.26.0",
    "mocha": "10.2.0",
    "nodemon": "2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "eslintConfig": {
    "env": {
      "node": true
    }
  },
  "bugs": {
    "url": "https://github.com/gear-tech/rosetta-api/issues"
  },
  "homepage": "https://github.com/gear-tech/rosetta-api/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gear-tech/rosetta-api"
  }
}
